<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <RecipeDef Name="DV_RecipeRoot" Abstract="True">
    <workAmount>350</workAmount>
    <workSpeedStat>DrugCookingSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Brewing</soundWorking>
  </RecipeDef>
  <RecipeDef Name="DV_RecipeBase" Abstract="True">
    <description>Use various powders and liquids to dye cloth.</description>
    <jobString>Dyeing cloth.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Cloth</li>
            <li>DV_UndyedCloth</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Cloth</li>
        <li>DV_UndyedCloth</li>
      </thingDefs>
    </fixedIngredientFilter>
  </RecipeDef>
  <RecipeDef ParentName="DV_RecipeRoot">
    <defName>DV_BleachCloth</defName>
    <label>bleach dyed cloth</label>
    <description>Bleach dyed cloth to standard cloth.</description>
    <jobString>Bleaching cloth.</jobString>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>DV_DyedCloth</li>
          </categories>
        </filter>
        <count>10</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>DV_DyedCloth</li>
      </categories>
    </fixedIngredientFilter>
    <products>
      <DV_UndyedCloth>10</DV_UndyedCloth>
    </products>
  </RecipeDef>
